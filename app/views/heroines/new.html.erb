<%= tag.h1 "Create a new heroine"%>





<%= form_for @heroine do |f| %>
<%= f.label :name %>
<%= tag.br %>
<%= f.text_field :name %>
<%= tag.br %>

<%= f.label :super_name %>
<%= tag.br %>
<%= f.text_field :super_name %>
<%= tag.br %>

<%= f.collection_select :power_id, @powers, :id, :name %>
<%= tag.br %>

<%= f.submit %>

<% end %>