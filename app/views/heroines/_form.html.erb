<% if flash[:error]%>
    <% flash[:error].each do |error|%>
        <%= error%>
    <%end%>
<%end%>
<%= form_for @heroine do |f|%>
<%= f.label :Name%>
<%= f.text_field :name %><br>

<%= f.label :Super_name%>
<%= f.text_field :super_name %><br>

<%= f.label :Power%>
<%= f.collection_select :power_id, @powers, :id, :name%><br>
<%=f.submit%>
<%end%>


